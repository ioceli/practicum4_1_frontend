{
  "name": "practicum",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "set NODE_OPTIONS=--openssl-legacy-provider && ng serve --configuration=test --host 0.0.0.0",
    "build": "ng build --outputHashing=all --prod --configuration=production",
    "dev:windows": "set NODE_OPTIONS=--openssl-legacy-provider && ng serve --configuration=local --host 0.0.0.0",
    "dev:mac": "export NODE_OPTIONS=--openssl-legacy-provider && ng serve --configuration=local --host 0.0.0.0",
    "test:mac": "export NODE_OPTIONS=--openssl-legacy-provider && ng serve --configuration=test --host 0.0.0.0",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.0",
    "@angular/cdk": "^18.0.0",
    "@angular/common": "^18.0.0",
    "@angular/compiler": "^18.0.0",
    "@angular/core": "^18.0.0",
    "@angular/forms": "^18.0.0",
    "@angular/material": "^17.0.5",
    "@angular/platform-browser": "^18.0.0",
    "@angular/platform-browser-dynamic": "^18.0.0",
    "@angular/router": "^18.0.0",
    "@ng-bootstrap/ng-bootstrap": "^16.0.0",
    "@popperjs/core": "^2.11.8",
    "angular2-smart-table": "^3.2.0",
    "bootstrap": "^5.3.2",
    "jw-bootstrap-switch-ng2": "^2.0.5",
    "keycloak-angular": "^15.0.0",
    "keycloak-js": "^22.0.5",
    "lodash-es": "^4.17.21",
    "moment": "^2.30.1",
    "ng2-nouislider": "^2.0.0",
    "ngx-cookie-service": "^17.0.1",
    "ngx-spinner": "^16.0.2",
    "ngx-toastr": "^18.0.0",
    "nouislider": "^15.7.1",
    "pdfmake": "^0.2.9",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primeng": "^17.3.2",
